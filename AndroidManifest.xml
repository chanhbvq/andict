<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="peacemoon.andict"
      android:versionCode="1" android:versionName="0.1">
    <application android:icon="@drawable/andict" android:label="@string/app_name">
        <activity android:name=".Andict"
                  android:label="@string/app_name" android:launchMode="standard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    	<activity android:name=".ContentView"></activity>
		<provider android:name="AndictProvider" android:authorities="peacemoon.andict.AndictProvider"></provider>
	<activity android:name=".MainPreferenceActivity" android:label="@string/app_name"></activity>
	<activity android:name=".GoogleTranslateActivity" android:label="Andict - Google Translation"></activity>
<activity android:name=".HistoryView"></activity>
</application>
	
	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-sdk android:minSdkVersion="3" />

<uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>
</manifest> 